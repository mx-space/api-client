{
  "name": "@mx-space/api-client",
  "version": "0.6.1",
  "description": "A api client for mx-space server@next",
  "author": "Innei",
  "license": "MIT",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "unpkg": "build/index.umd.min.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bump": {
    "before": [
      "git pull --rebase",
      "pnpm i",
      "npm run package"
    ],
    "after": [
      "npm publish --access=public"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "files": [
    "build",
    "lib",
    "esm",
    "readme.md",
    "tsconfig.json"
  ],
  "engines": {
    "pnpm": ">=6"
  },
  "scripts": {
    "prepare": "husky install",
    "predeploy": "rm -rf dist",
    "prebuild": "rm -rf rm -rf lib && rm -rf esm",
    "build": "ttsc --build src/tsconfig.build.json && ttsc --build src/tsconfig.cjs.json",
    "package": "NODE_ENV=production npm run build && rollup -c",
    "postpackage": "dts-bundle-generator -o build/index.d.ts src/index.ts --project tsconfig.json  --no-check",
    "prepackage": "rm -rf build",
    "release": "vv",
    "dev": "vite",
    "build:vite": "vite build",
    "preview": "vite preview --port 2323",
    "deploy": "vite build && gh-pages -d dist",
    "test": "jest --env=jsdom --colors --coverage test",
    "test:watch": "jest --env=jsdom --colors --coverage test --watch -i --detectOpenHandles"
  },
  "devDependencies": {
    "@innei-util/eslint-config-ts": "latest",
    "@innei-util/prettier": "latest",
    "@innei/bump-version": "0.2.2",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.21",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@zerollup/ts-transform-paths": "1.7.18",
    "axios": "0.25.0",
    "cors": "2.8.5",
    "dts-bundle-generator": "6.4.0",
    "esbuild": "0.14.15",
    "express": "4.17.2",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.2.2",
    "lodash": "4.17.21",
    "prettier": "2.5.1",
    "react": "18.0.0-rc.0-next-fe905f152-20220107",
    "react-dom": "18.0.0-rc.0-next-fe905f152-20220107",
    "react-json-view": "1.21.3",
    "rollup": "2.66.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.1.3",
    "tslib": "2.3.1",
    "ttypescript": "1.5.13",
    "typescript": "4.4.4",
    "umi-request": "1.4.0",
    "vite": "2.7.13",
    "vite-tsconfig-paths": "3.3.17"
  },
  "dependencies": {
    "camelcase-keys": "7.0.1"
  }
}
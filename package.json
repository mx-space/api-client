{
  "name": "@mx-space/api-client",
  "version": "0.0.2",
  "description": "A api client for mx-space server@next",
  "author": "Innei",
  "license": "MIT",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "unpkg": "build/index.umd.min.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bump": {
    "before": [
      "git pull --rebase",
      "pnpm i",
      "npm run package"
    ],
    "after": [
      "sh ./scripts/create-tags.sh",
      "npm publish --access=public"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache"
    ]
  },
  "files": [
    "build",
    "lib",
    "esm",
    "readme.md",
    "tsconfig.json"
  ],
  "engines": {
    "pnpm": ">=6"
  },
  "scripts": {
    "prepare": "husky install",
    "predeploy": "rm -rf dist",
    "build": "tsc --build src/tsconfig.build.json && tsc --build src/tsconfig.cjs.json",
    "package": " NODE_ENV=production npm run build && rollup -c",
    "prepackage": "rm -rf lib && rm -rf build && rm -rf esm",
    "release": "vv",
    "dev": "vite",
    "build:vite": "vite build",
    "preview": "vite preview --port 2323",
    "deploy": "vite build && gh-pages -d dist",
    "test": "jest --env=jsdom --colors --coverage test",
    "test:watch": "jest --env=jsdom --colors --coverage test --watch"
  },
  "devDependencies": {
    "@innei-util/eslint-config-ts": "latest",
    "@innei-util/prettier": "latest",
    "@innei/bump-version": "0.1.5",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.176",
    "@types/node": "16.11.6",
    "axios": "0.24.0",
    "esbuild": "0.13.12",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "rollup": "2.58.3",
    "rollup-plugin-dts": "4.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.0.7",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "vite": "2.6.13",
    "vite-tsconfig-paths": "3.3.17"
  },
  "dependencies": {
    "camelcase-keys": "7.0.1"
  }
}